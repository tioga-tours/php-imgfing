{
  "name": "tioga-tours/php-imgfing",
  "description": "Check images on their unique fingerprint and compare them",
  "type": "library",
  "keywords": [
    "image processing"
  ],
  "license": "GNU v3",
  "authors": [
    {
      "name": "Martin Winkel",
      "email": "martin@tiogatours.nl",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/tioga-tours/php-imgfing/issues"
  },
  "require": {
    "php": ">=7.4",
    "ext-gd": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd": "~2.9",
    "friendsofphp/php-cs-fixer": "~2.16",
    "phploc/phploc": "~7.0",
    "sebastian/phpcpd": "~6.0",
    "squizlabs/php_codesniffer": "~3.3"
  },
  "suggest": {
    "ext-imagick": "*"
  },
  "autoload": {
    "psr-4": {
      "ImgFing\\": [
        "src"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
